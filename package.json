{
	"name": "carta-plugin-imsize",
	"version": "2.1.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"license": "MIT",
	"author": "maisonsmd <maisonsmd@gmail.com>",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "husky install",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/maisonsmd/carta-plugin-imsize.git"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"carta-md": "^4.0.0",
		"unified": "^11.0.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/package": "^2.3.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/mdast": "^4.0.4",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"carta-md": "^4.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unified": "^11.0.4",
		"vite": "^5.4.0"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": "eslint --cache --fix --no-error-on-unmatched-pattern",
		"*.{js,ts,css,md,svelte,html,yml,json,cjs}": "prettier --write"
	},
	"svelte": "./dist/index.js",
	"keywords": [
		"carta",
		"markdown",
		"editor",
		"remark",
		"unified",
		"text editor",
		"syntax highlighting",
		"image",
		"resize"
	],
	"description": "This plugin adds ability to render images to specific sizes",
	"bugs": {
		"url": "https://github.com/maisonsmd/carta-plugin-imsize/issues"
	},
	"homepage": "https://github.com/maisonsmd/carta-plugin-imsize#readme",
	"dependencies": {
		"mdast": "^3.0.0",
		"mdast-util-to-string": "^4.0.0",
		"unist-util-find-after": "^5.0.0",
		"unist-util-find-between-all": "^1.0.5",
		"unist-util-visit": "^5.0.0"
	}
}
